{
  "compilerOptions": {
    "target": "ES2016",
    "module": "ESNext",
    "rootDir": "./",
    "outDir": "./dist",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "paths": {
      "@prisma/client": ["/node_modules/@prisma/client"]
    },
    "forceConsistentCasingInFileNames": true,  
    "strict": true,
    "skipLibCheck": true
  }
}